/*
1      2       3      4      5     6     7     8      9
       A       B     C     D     E     F    G
                 3     4      5     6     7
                        C     D    E
                                5


*/

public class pattern22
{
	public static void main(String x[])
	{
		
		for(int i=1; i<=5; i++)
		{   int k=1;
			
			for(int j=1; j<=9; j++)
			{
				boolean flag=true;
				char ch='A';
				if(flag && j>=i && j<=10-i)
				{
					System.out.print(k++);
					flag=false;
				}
				else
				{
					System.out.print(ch++);
					flag=true;
				}
			}
			System.out.println();
		}
	}
}